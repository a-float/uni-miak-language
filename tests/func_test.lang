int sum = (int a, int b) -> a + b;

int fib = (int n) -> {
    while true{
        if n == 0 or n == 1{
            ret n;
        }
        else {
            ret sum(fib(n-1), fib(n-2));
        }
    }
}

for(int i in 1::sum(2,0)*2){
    print "fib(" + i + ") is " + fib(i);
}

